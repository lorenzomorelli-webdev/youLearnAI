# OpenAI API Key - Required for GPT summarization and Whisper transcription
# Get your API key from https://platform.openai.com/
OPENAI_API_KEY=your_openai_api_key_here

# Deepseek API Key - Optional, for alternative summarization
# Get your API key from https://deepseek.com/
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Telegram Bot Token - Required
# Get your token from @BotFather on Telegram
TELEGRAM_TOKEN=your_telegram_bot_token_here

# Proxy URL - Optional, helps bypass YouTube restrictions
PROXY_URL=your_proxy_url_here


# Whitelist configuration
ALLOWED_USERS=1234567890,9876543210